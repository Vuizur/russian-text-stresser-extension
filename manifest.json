{
    "manifest_version": 2,
    "name": "Russian Stresser",
    "version": "1.0",
    "description": "A Chrome extension that stresses Russian text on web pages using a local server.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "http://localhost:8000/*"
    ]
}